    <!-- actions opening views on models -->

    <record model="ir.actions.act_window" id="thewalkingdead.outposts_action_window">
      <field name="name">thewalkingdead outposts</field>
      <field name="res_model">thewalkingdead.outpost</field>
      <field name="view_mode">tree,form</field>
    </record>
    <record model="ir.actions.act_window" id="thewalkingdead.b_types_action_window">
      <field name="name">thewalkingdead Building types</field>
      <field name="res_model">thewalkingdead.building_type</field>
      <field name="view_mode">tree,form</field>
    </record>
    <record model="ir.actions.act_window" id="thewalkingdead.buildings_action_window">
      <field name="name">thewalkingdead Buildings</field>
      <field name="res_model">thewalkingdead.building</field>
      <field name="view_mode">tree,form</field>
    </record>
    <record model="ir.actions.act_window" id="thewalkingdead.survivors_action_window">
      <field name="name">thewalkingdead Survivors</field>
      <field name="res_model">thewalkingdead.survivor</field>
      <field name="view_mode">tree,form</field>
    </record>
    <record model="ir.actions.act_window" id="thewalkingdead.vehicles_action_window">
      <field name="name">thewalkingdead Vehicles</field>
      <field name="res_model">thewalkingdead.vehicle</field>
      <field name="view_mode">tree,form</field>
    </record>


    <record model="ir.actions.act_window" id="thewalkingdead.players_action_window">
      <field name="name">thewalkingdead Players</field>
      <field name="res_model">thewalkingdead.player</field>
      <field name="view_mode">tree,form</field>
    </record>
     <record model="ir.actions.act_window" id="thewalkingdead.roads_action_window">
      <field name="name">thewalkingdead Roads</field>
      <field name="res_model">thewalkingdead.road</field>
      <field name="view_mode">tree,form</field>
    </record>

        <record model="ir.actions.act_window" id="thewalkingdead.character_template_action_window">
      <field name="name">thewalkingdead Character Templates</field>
      <field name="res_model">character_template</field>
      <field name="view_mode">tree,form</field>
    </record>
