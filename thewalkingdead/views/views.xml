        }
      </field>
    </record>
-->

    <!-- Top menu item -->

    <menuitem name="thewalkingdead" id="thewalkingdead.menu_root" />

    <!-- menu categories -->

    <menuitem name="Game" id="thewalkingdead.outposts" parent="thewalkingdead.menu_root" />
    <menuitem name="Game aux" id="thewalkingdead.aux" parent="thewalkingdead.menu_root" />
    <menuitem name="Clients" id="thewalkingdead.clients" parent="thewalkingdead.menu_root" />


    <!-- actions -->

    <menuitem name="outposts" id="thewalkingdead.outposts_list" parent="thewalkingdead.outposts" action="thewalkingdead.outposts_action_window" />
    <menuitem name="Building Types" id="thewalkingdead.building_types_list" parent="thewalkingdead.outposts" action="thewalkingdead.b_types_action_window" />
    <menuitem name="Buildings" id="thewalkingdead.buildings_list" parent="thewalkingdead.outposts" action="thewalkingdead.buildings_action_window" />
    <menuitem name="Survivors" id="thewalkingdead.survivors_list" parent="thewalkingdead.outposts" action="thewalkingdead.survivors_action_window" />
    <menuitem name="Roads" id="thewalkingdead.roads_list" parent="thewalkingdead.outposts" action="thewalkingdead.roads_action_window" />
    <menuitem name="travels" id="thewalkingdead.travel_list" parent="thewalkingdead.outposts" action="thewalkingdead.travel_action_window" />

    <menuitem name="Players" id="thewalkingdead.playerss_list" parent="thewalkingdead.clients" action="thewalkingdead.players_action_window" />

<!--<menuitem name="Charater Templates" id="thewalkingdead.character_template" parent="thewalkingdead.aux" action="thewalkingdead.character_template_action_window" /> -->
  </data>
